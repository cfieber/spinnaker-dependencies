<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.netflix.spinnaker</groupId>
  <artifactId>spinnaker-dependencies</artifactId>
  <version>@version@</version>
  <properties>
    <spring.platform.version>1.1.5.RELEASE</spring.platform.version>
    <aws.sdk.version>1.11.39</aws.sdk.version>
    <spectator.version>0.41.0</spectator.version>
    <eureka.version>1.5.6</eureka.version>
    <archaius.version>0.7.5</archaius.version>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>io.spring.platform</groupId>
        <artifactId>platform-bom</artifactId>
        <version>${spring.platform.version}</version>
        <scope>import</scope>
        <type>pom</type>
      </dependency>
      <dependency>
        <groupId>com.amazonaws</groupId>
        <artifactId>aws-java-sdk-bom</artifactId>
        <version>${aws.sdk.version}</version>
        <scope>import</scope>
        <type>pom</type>
      </dependency>
      <dependency>
        <groupId>com.netflix.spectator</groupId>
        <artifactId>spectator-api</artifactId>
        <version>${spectator.version}</version>
      </dependency>
      <dependency>
        <groupId>com.netflix.spectator</groupId>
        <artifactId>spectator-ext-aws</artifactId>
        <version>${spectator.version}</version>
      </dependency>
      <dependency>
        <groupId>com.netflix.spectator</groupId>
        <artifactId>spectator-ext-jvm</artifactId>
        <version>${spectator.version}</version>
      </dependency>
      <dependency>
        <groupId>com.netflix.spectator</groupId>
        <artifactId>spectator-ext-gc</artifactId>
        <version>${spectator.version}</version>
      </dependency>
      <dependency>
        <groupId>com.netflix.eureka</groupId>
        <artifactId>eureka-core</artifactId>
        <version>${eureka.version}</version>
      </dependency>
      <dependency>
        <groupId>com.netflix.eureka</groupId>
        <artifactId>eureka-client</artifactId>
        <version>${eureka.version}</version>
      </dependency>
      <dependency>
        <groupId>com.netflix.archaius</groupId>
        <artifactId>archaius-core</artifactId>
        <version>${archaius.version}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>
</project>
